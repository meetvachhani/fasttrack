#include<stdio.h>
void input(int a[],int *n,int *k)
{
    printf("Enter the number of elements : ");
    scanf("%d",n);
    printf("Enter the elements : \n");
    for(int i=1;i<=*n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter the key element : ");
    scanf("%d",k);
}
void l_search(int a[],int n,int k,int *p,int *q)
{
    *p=1;
    for(int i=0;i<n;i++)
    {
        *p++;
        if(k==a[i])
        {
            *q=1;
            break;
        }
        
    }
}
void output(int a[],int p,int q,int k)
{
    if(q==1)
    {
        printf("Key Element %d is found at location : %d",k,p);
    }
    else
    {
        printf("Key Element is not present !!");
    }
}
int main()
{
    int a[50],n,k,p,q=0;
    input(a,&n,&k);
    l_search(a,n,k,&p,&q);
    output(a,p,q,k);
}
