#include <stdio.h>
int input(int x[],int *n)
{
    printf("Enter number of elements : ");
    scanf("%d", &*n);
    printf("Enter %d integers :\n",*n);
    for (int i=0;i<*n;i++)
    {
        scanf("%d", &x[i]);
    }
}
void bubble_sort(int a[], int n)
{
    int sub;
    for (int i=0;i<n-1;i++)
    { 
        for (int d=0;d<n-i-1;d++)
        {
            if (a[d]>a[d+1])
            {
                sub=a[d];
                a[d]=a[d+1];
                a[d+1]=sub;
            }
        }
    }
}
void sorted_list(int a[],int n)
{
    printf("Sorted list in ascending order:\n");
    for (int i=0;i<n;i++)
    {
        printf("%d\n", a[i]);
    }
}
int main()
{
    int a[100],n;
    input(a,&n);
    bubble_sort(a,n);
    sorted_list(a,n);
}
